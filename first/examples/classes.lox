class WithProperties {
}

var c = WithProperties();

c.test = 10;

print c.test;


class Bacon {
  eat() {
    print "Crunch crunch crunch!";
  }
}

Bacon().eat();

class WithThis {
  useThis() {
    return this.test;
  }
}

var withThis = WithThis();
withThis.test = "'this' works!";

print withThis.useThis();

class Rectangle {
  init(a, b) {
    this.a = a;
    this.b = b;
  }

  area() {
    return a * b;
  }
}

var rect = Rectangle(3, 5);
print rect.area();
